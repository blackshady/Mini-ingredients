{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "import-name": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "align": [
      true,
      "parameters",
      "arguments",
      "statements"
    ]
  },
  "rulesDirectory": []
}

